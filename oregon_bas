5922 PRINT D3; mes$; S_YEAR$: REM Spanish
5925 PRINT
5930 REM LINE 5930 NOTE 1
5940 IF B>0 THEN GOTO 5960
5950 LET B=0
5960 IF C>0 THEN GOTO 5980
5970 LET C=0
5980 IF M1>0 THEN GOTO 6000
5990 LET M1=0
6000 IF T>0 THEN GOTO 6020
6010 LET T=0
6020 IF F>0 THEN GOTO 6040
6030 LET F=0
6040 REM LINES 6040-6045 NOTE 1
6041 PRINT S_FOOD$ ;F
6042 PRINT S_BULLETS$ ;B
6043 PRINT S_CLOTHING$ ;C
6044 PRINT S_MISC_SUPPLIES$ ;M1
6045 PRINT S_CASH$ ;T
6050 GOSUB 7000
6060 PRINT S_END_01$
6062 PRINT
6064 PRINT S_END_02$
6066 PRINT
6068 PRINT S_END_03$
6070 PRINT S_END_04$
6080 PRINT
6090 PRINT S_END_05$
6095 PRINT S_END_06$
6100 PRINT
6110 PRINT S_END_07$
6120 GOTO 7100

6130 REM ***SHOOTING SUB-ROUTINE***
6131 REM THE METHOD OF TIMING THE SHOOTING (LINES 6210-6240)
6132 REM WILL VARY FROM SYSTEM TO SYSTEM. FOR EXAMPLE, H-P
6133 REM USERS WILL PROBABLY PREFER TO USE THE 'ENTER' STATEMENT.
6134 REM IF TIMING ON THE USER'S SYSTEM IS HIGHLY SUSCEPTIBLE
6135 REM TO SYSTEM RESPONSE TIME, THE FORMULA IN LINE 6240 CAN
6136 REM BE TAILORED TO ACCOMMODATE THIS BY EITHER INCREASING
6137 REM OR DECREASING THE 'SHOOTING' TIME RECORDED BY THE SYSTEM
6140 DIM S$(4,4)
6150 S$(1)="bang"
6160 S$(2)="blam"
6170 S$(3)="pow"
6180 S$(4)="wham"
6182 B3 = date
6185 for a=1 to 4
6187   S6=INT(RND*4+0.5)
6188   if S6=0 then goto 6187
6190   PRINT S_TYPE$ ; S$(S6)
6192   INPUT Z$
6195   IF Z$<>S$(S6) THEN b3=b3-9
6197 next a
6200 B2 = date
6210 B1 = B2 - B3
6241 REM  /50 - (D9-1)
6242 rem PRINT "B3=";B3
6243 rem PRINT "B2=";B2
6244 rem PRINT "Seconds=";B1
6245 rem PRINT "DIFICULT=";D9
6246 B1= int (B1 - (D9*2))
6247 rem PRINT "Result=";B1
6248 rem GOSUB 7000
6250 PRINT
6255 IF B1>0 THEN GOTO 6260
6257 B1=0
6260 if B1>9 then B1=9
6280 RETURN
6282 REM so B1 = 0 => to fast
6285 REM    B1 = 9 => not correct
6287 REM    1 <= B1 <= 8 diferent quality.

6290 REM ***ILLNESS SUB-ROUTINE***
6300 IF 100*RND<10+35*(E-1) THEN GOTO 6370
6309 REM LINE 6310 NOTE 1
6310 IF 100*RND<100-(40/4^(E-1)) THEN GOTO 6410
6320 PRINT S_ILL_SERIOUS$
6330 PRINT S_ILL_MEDICAL$
6340 M1=M1-10
6350 S4=1
6360 GOTO 6440
6370 PRINT S_ILL_WILD$
6380 M=M-5
6390 M1=M1-2
6400 GOTO 6440
6410 PRINT S_ILL_BAD$
6420 M=M-5
6430 M1=M1-5
6440 IF M1<0 THEN GOTO 5110
6450 IF L1=1 THEN GOTO 4940
6460 GOTO 4710
6800 END

6900 REM Routines by Alea not in original

7000 PRINT
7010 PRINT S_PRESS_KEY$
7020 REPEAT wait
7021 IF INKEY$<>"" THEN EXIT wait
7022 REM WAIT Until the key isn't pressed any more.
7025 end rep wait
7030 PAUSE 0 : PAUSE 0
7040 CLS
7060 RETURN

7100 PRINT
7110 PRINT S_OTHER_PLAY$
7120 INPUT Z$
7130 if Z$<>"N" AND Z$<>"n" THEN GOTO 10
7140 NEW

9000 REM Language Selection
9010 print "Pulsa 1 para idioma Espanol"
9020 print "Press 2 for English Language"
9030 repeat x
9040   a$=inkey$
9050   if a$="1" then gosub 11000 : exit x
9060   if a$="2" then gosub 10000 : exit x
9070 end repeat x
9080 CLS
9090 return

