90 REM ***IDENTIFICATION OF VARIABLES IN THE PROGRAM***
91 LET A% =0 : rem as Integer : REM = AMOUNT SPENT ON ANIMALS
92 LET B% =0 : rem  as Integer : REM  = AMOUNT SPENT ON AMMUNITION
93 REM LET B1 as Float : REM = ACTUAL RESPONSE TIME FOR INPUTTING "BANG"
94 REM LET B2 as Float : REM = CLOCK TIME AT END OF INPUTTING "BANG"
95 REM LET B3 as Float : REM = CLOCK TIME AT START OF INPUTTING "BANG"
96 LET C% =0 : rem  as Integer : REM  = AMOUNT SPENT ON CLOTHING
97 LET C1% =0: rem  as uByte : REM = FLAG FOR INSUFFICIENT CLOTHING IN COLD WEATHER
98 REM Z$ = YES/N0 RESPONSE TO QUESTIONS
99 LET D1% =0: rem  as uByte : REM = COUNTER IN GENERATING EVENTS
100 LET D3% =0: rem  as uByte : REM = TURN NUMBER FOR SETTING DATE
101 REM D4 = CURRENT DATE
102 LET D9% =0: rem  as uByte : REM = CHOICE OF SHOOTING EXPERTISE LEVEL
103 LET E% =0: rem  as uByte : REM = CHOICE OF EATING
104 LET F% =0 : rem  as Integer : REM  = AMOUNT SPENT ON FOOD
105 LET F1% =0: rem  as uByte : REM = FLAG FOR CLEARING SOUTH PASS
106 LET F2% =0: rem  as uByte : REM = FLAG FOR CLEARING BLUE MOUNTAINS
107 REM LET F9 as Float : REM = FRACTION OF 2 WEEKS TRAVELED ON FINAL TURN
108 LET K8% =0: rem  as uByte : REM = FLAG FOR INJURY
109 LET L1% =0: rem  as uByte : REM = FLAG FOR BLIZZARD
110 LET M% =0:  rem as uInteger : REM = TOTAL MILEAGE WHOLE TRIP
111 LET M1% =0 : rem  as Integer : REM  = AMOUNT SPENT ON MISCELLANEOUS SUPPLIES
112 LET M2% =0 : rem  as Integer : REM = TOTAL MILEAGE UP THROUGH PREVIOUS TURN
113 REM LET M9% =0: rem  as uByte : REM = FLAG FOR CLEARING SOUTH PASS IN SETTING MILEAGE
114 LET P% =0 : rem  as integer : REM = AMOUNT SPENT ON ITEMS AT FORT
115 REM LET R1 as float : REM = RANDOM NUMBER IN CHOOSING EVENTS
116 LET S4% =0: rem  as uByte : REM = FLAG FOR ILLNESS
117 LET S5% =0 : rem  as Integer : REM = ""HOSTILITY OF RIDERS"" FACTOR
118 LET S6% =0: rem  as uByte : REM = SHOOTING WORD SELECTOR
119 REM S$ = VARIATIONS OF SHOOTING WORD
120 LET T% =0 : rem  as Integer : REM  = CASH LEFT OVER AFTER INITIAL PURCHASES
121 LET T1% =0: rem  as uByte : REM = CHOICE OF TACTICS WHEN ATTACKED
122 LET X% =0: rem  as uByte : REM = CHOICE OF ACTION FOR EACH TURN
123 LET X1% =0: rem  as uByte : REM = FLAG FOR FORT OPTION155 REM
125 REM LET mes$ as String
126 REM LET temp1 as Float
127 REM LET temp2 as Float

130 mode 8
131 OPEN #1,con_512x256a0x0
132 rem WINDOW #1,256,256,0,0
150 INK 4: PAPER 0: CLS: pause 1 : cls
155 gosub 9000 : REMark Language Selection
160 PRINT S_NEED_INST$ ;
170 REM DIM C$(5)
180 REM RANDOMIZE REMOVED
190 INPUT Z$
200 IF (Z$="N") THEN GOTO 690
205    IF (Z$="n") THEN GOTO 690
210 PRINT
220 PRINT

230 REM ***INSTRUCTIONS********************
240 PRINT S_I_001$
245 PRINT S_I_002$
